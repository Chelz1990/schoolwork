#include <iostream>

using namespace std;

int main() {
    int a, b, c , d, f, sum;
    float average;
    cout << "Please enter up to five of your latest test scores." << endl;
    cin >> a >> b >> c >>  d >> f;
    sum=a + b + c + d + f;
    average=sum/5;
    cout << "Your average score is: " << average << endl;

    char grade;
    if (average>=90)
        grade='A';
    else if (average>=80)
        grade='B';
    else if (average>=70)
        grade='C';
    else if (average>=60)
        grade='D';
    else
        grade='F';

    cout << "Your grade is " << grade << endl;
}
